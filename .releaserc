{
  "branches": ["main"],

  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@semantic-release/git", {
      "assets": [
        "all/pom.xml",
        "core/pom.xml",
        "ui.apps/pom.xml",
        "ui.apps.structure/pom.xml",
        "ui.config/pom.xml",
        "CHANGELOG.md",
        "pom.xml"
      ]
    }],
    ["@semantic-release/github", {
      "assets": [
        {
          "path": "all/target/aem-vite.all-${nextRelease.version}.zip"
        },
        {
          "path": "core/target/aem-vite.core-${nextRelease.version}.jar"
        }
      ]
    }],
    "@conveyal/maven-semantic-release"
  ]
}